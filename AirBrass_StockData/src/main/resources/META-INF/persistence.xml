<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
    <persistence-unit name="AirBrass_StockData" transaction-type="RESOURCE_LOCAL">
    <provider>org.hibernate.ejb.HibernatePersistence</provider>
	<class>fr.afcepf.al30.stock.entity.Produit</class>
	<class>fr.afcepf.al30.stock.entity.Utilisateur</class>
	<class>fr.afcepf.al30.stock.entity.Commande</class>
	<class>fr.afcepf.al30.stock.entity.LigneCommande</class>
	<class>fr.afcepf.al30.stock.entity.Fournisseur</class>
	<properties>
			<property name="hibernate.show_sql" value="false"/>
      <property name="javax.persistence.provider" value="org.hibernate.jpa.HibernatePersistenceProvider"/>
      <property name="hibernate.dialect" value="org.hibernate.dialect.MySQLDialect"/>
	</properties>
	</persistence-unit>
</persistence>
